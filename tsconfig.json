{
  "compilerOptions": {
      "strict": true,
      "target": "es5",
      "removeComments": true,
      "noUnusedLocals": true,
      "noUnusedParameters": true,
      "strictPropertyInitialization": false,
      "sourceMap": true,
      "outFile": "docs/js/main.js",
      "allowJs": true
  },
  "exclude": [
    "node_modules",
    "docs"
    ],
  "include": [
      "dev/**/*"
  ]
}